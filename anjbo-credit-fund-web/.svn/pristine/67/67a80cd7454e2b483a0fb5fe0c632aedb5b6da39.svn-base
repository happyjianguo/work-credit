<script src="./js/mui.min.js"></script>
<script src="./js/mui.picker.min.js"></script>
<script src="./js/jquery.min.js"></script>
<script>
  (function($, doc, $$) {
				$.init();
				$.ready(function() {

				 
				   $$(".sel2").click(function(){
					  var userPicker = new $.PopPicker();
					  userPicker.setData([{
						value: 'yes',
						text: '男'
					  }, {
						value: 'no',
						text: '女'
					  }]);
					  var _this=$$(this);
				      userPicker.show(function(items) {
							_this.val((items[0].text));
							//返回 false 可以阻止选择框的关闭
							//return false;
						});
				  });
				 


					 
					 
				});
			})(mui, document,jQuery);
</script>
