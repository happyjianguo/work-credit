<div class="lhw-alert-bg"  ng-show="isOrdinaryShow">
    <div class="lhw-alert-box" style="width:1200px;">
        <div class="lhw-alert-tit relative" >以下信息推送给资金方<div class="lhw-x" ng-click="isOrdinaryShow=false;"></div></div>
        <div style="width:100%;height:490px; overflow-y:scroll;">
            <div class="row">
				<div class="col-md-12" style="padding-top:10px;">
					<ul class="nav nav-tabs">
						<li ng-class="{'active':ordinaryTitle=='orderOrdinaryShow'}">
							<a href="javascript:void(0);" ng-click="ordinaryTitle='orderOrdinaryShow'">借款信息</a>
						</li>
						<li ng-class="{'active':ordinaryTitle=='auditFirstOrdinaryShow'}" ng-show="isAuditShow">
							<a href="javascript:void(0);" ng-click="ordinaryTitle='auditFirstOrdinaryShow'">审批意见</a>
						</li>
						<li ng-class="{'active':ordinaryTitle=='imageOrdinaryShow'}">
							<a href="javascript:void(0);" ng-click="ordinaryTitle='imageOrdinaryShow'">影像资料</a>
						</li>
					</ul>
				</div>
			</div>
			<div ng-show="ordinaryTitle=='orderOrdinaryShow'"  style="width:100%;height:490px; overflow-y:scroll;">
				<allocation-fund-ordinary-order-detail></allocation-fund-ordinary-order-detail>
			</div>
			<div ng-show="ordinaryTitle=='auditFirstOrdinaryShow'"  style="width:100%;height:490px; overflow-y:scroll;">
				<allocation-fund-ordinary-audit-detail></allocation-fund-ordinary-audit-detail>
			</div>
			<div ng-show="ordinaryTitle=='imageOrdinaryShow'"  style="width:100%;height:490px; overflow-y:scroll;">
				<allocation-fund-ordinary-businfo-detail></allocation-fund-ordinary-businfo-detail>
			</div>
            
        </div>

        <div class="lhw-alert-but">
            <button type="button" class="btn btn-primary lhw-alert-ok" ng-click="ordinaryShow(false)" style="margin-right:80px;">关闭</button>
        </div>
    </div>
</div>