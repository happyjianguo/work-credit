<div class="lhw-alert-bg"  ng-show="isYntrustShow">
    <div class="lhw-alert-box" style="width:1200px;">
        <div class="lhw-alert-tit relative" >以下信息推送给资金方<div class="lhw-x" ng-click="isYntrustShow=false;"></div></div>
        <div style="width:100%;height:490px;">
            <div class="row">
				<div class="col-md-12" style="padding-top:10px;">
					<ul class="nav nav-tabs">
						<li ng-class="{'active':yntrustTitle=='yntrustOrderShow'}">
							<a href="javascript:void(0);" ng-click="yntrustTitle='yntrustOrderShow'">借款人信息</a>
						</li>
						<li ng-class="{'active':yntrustTitle=='yntrustContractShow'}">
							<a href="javascript:void(0);" ng-click="yntrustTitle='yntrustContractShow'">合同信息</a>
						</li>
						<li ng-class="{'active':yntrustTitle=='yntrustBusinfoShow'}">
							<a href="javascript:void(0);" ng-click="yntrustTitle='yntrustBusinfoShow'">影像资料</a>
						</li>
					</ul>
				</div>
			</div>
			
			<div ng-show="yntrustTitle=='yntrustOrderShow'"  style="width:100%;height:438px; overflow-y:scroll;">
				<allocation-fund-yntrust-order-detail></allocation-fund-yntrust-order-detail>
				<div style="width:100%;clear:both; height:52px"></div>
			</div>
			<div ng-show="yntrustTitle=='yntrustContractShow'"  style="width:100%;height:438px; overflow-y:scroll;">
				<allocation-fund-yntrust-contract-detail></allocation-fund-yntrust-contract-detail>
				<div style="width:100%;clear:both; height:52px"></div>
			</div>
			<div ng-show="yntrustTitle=='yntrustBusinfoShow'"  style="width:100%;height:438px; overflow-y:scroll;">
				<allocation-fund-yntrust-businfo-detail></allocation-fund-yntrust-businfo-detail>
				<div style="width:100%;clear:both; height:52px"></div>
			</div>
        </div>
        <div class="lhw-alert-but">
            <button type="button" class="btn btn-primary lhw-alert-ok" ng-click="yntrustShow(false)" style="margin-right:80px;">关闭</button>
        </div>
    </div>
</div>