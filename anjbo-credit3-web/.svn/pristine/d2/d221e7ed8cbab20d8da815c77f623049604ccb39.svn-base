<form name="huarongKgAppointForm">
<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;快鸽提单信息--业务信息</div>
    <div class="row form-inline detail-row" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>业务类型: </label> <span ng-bind="kgAppoint.businessType"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>申请人姓名:</label>
            <span ng-bind="kgAppoint.custName"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>借款金额:</label>
            <span ng-bind="kgAppoint.loanAmount"></span>&nbsp;万元
        </div>
        <div class="col-md-6 col-lg-4">
            <label>期限:</label>
            <span ng-bind="kgAppoint.term"></span>天
        </div>
        <div class="col-md-6 col-lg-4">
            <label>费率:</label>
            <span ng-bind="kgAppoint.rate"></span>%/天
        </div>
        <div class="col-md-6 col-lg-4">
            <label>逾期费率:</label>
            <span ng-bind="kgAppoint.overdueTate"></span>%/天
        </div>
    </div>
</div>
<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;原贷款信息</div>
    <div class="row form-inline detail-row w112" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>原房贷金额:</label>
            <span ng-bind="kgAppoint.yOriLenAmount"></span>&nbsp;万元
        </div>
        <div class="col-md-6 col-lg-4">
            <label>贷款余额:</label>
            <span ng-bind="kgAppoint.yLoanBalance"></span>&nbsp;万元
        </div>
        <div class="col-md-6 col-lg-4">
            <label>原贷款是否银行:</label>
            <span ng-bind="kgAppoint.yIsBank"></span>
        </div>
        <div class="col-md-12 col-lg-12" ng-if="kgAppoint.yIsBank=='是'">
            <label style="width:120px">原贷款银行-支行: </label>
            <span ng-bind="kgAppoint.yOriBank"></span>
        </div>
        <div class="col-md-6 col-lg-4" ng-if="kgAppoint.yIsBank=='否'">
            <label>贷款地址: </label>
            <span ng-bind="kgAppoint.yOriBank"></span>
        </div>
    </div>
</div>
    <div>
        <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;新贷款信息</div>
        <div class="row form-inline detail-row" style="padding:0 10px;">
            <div class="col-md-6 col-lg-4">
                <label>贷款金额:</label>
                <span ng-bind="kgAppoint.xLoanAmount"></span>&nbsp;万元
            </div>
            <div class="col-md-6 col-lg-4">
                <label>是否银行:</label>
                <span ng-bind="kgAppoint.xIsBank"></span>
            </div>
            <div class="col-md-12 col-lg-12" ng-if="kgAppoint.xIsBank=='是'">
                <label>贷款银行-支行: </label>
                <span ng-bind="kgAppoint.xLoanBank"></span>
            </div>
            <div class="col-md-6 col-lg-4" ng-if="kgAppoint.xIsBank=='否'">
                <label>贷款地址: </label>
                <span ng-bind="kgAppoint.xLoanBank"></span>
            </div>
        </div>
    </div>
<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;放款卡信息</div>
    <div class="row form-inline detail-row" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>账户类型:</label>
            <span ng-bind="kgAppoint.fAccountType"></span>
        </div>
        <div class="col-md-6 col-lg-8">
            <label>开户行: </label>
            <span ng-bind="kgAppoint.fOpenBank"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>户名: </label>
            <span ng-bind="kgAppoint.fOpenName"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>账号: </label>
            <span ng-bind="kgAppoint.fAccountNum"></span>
        </div>
        <div class="col-md-6 col-lg-4" ng-if="kgAppoint.fAccountType=='个人'">
            <label>证件号: </label>
            <span ng-bind="kgAppoint.fCaacNo"></span>
        </div>
    </div>
</div>

<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;回款卡信息</div>
    <div class="row form-inline detail-row" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>账户类型: </label>
            <span ng-bind="kgAppoint.hAccountType"></span>
        </div>
        <div class="col-md-6 col-lg-8">
            <label>开户行: </label>
            <span ng-bind="kgAppoint.hOpenBank"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>户名: </label>
            <span ng-bind="kgAppoint.hOpenName"></span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>账号: </label>
            <span ng-bind="kgAppoint.hAccountNum"></span>
        </div>
        <div class="col-md-6 col-lg-4" ng-if="kgAppoint.hAccountType=='个人'">
            <label>证件号: </label>
            <span ng-bind="kgAppoint.hCaacNo"></span>
        </div>
    </div>
</div>
</form>