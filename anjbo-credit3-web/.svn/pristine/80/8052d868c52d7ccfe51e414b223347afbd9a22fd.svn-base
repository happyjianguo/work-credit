<form ng-show="order.customerType==2" method="post" name="customerShareholderForm" id="customerShareholderForm" class="form-inline detail-row w150 customerfrom-supplement">	
	<div class="col-md-12 detail-right-tit ">&nbsp;&nbsp;  企业信息(非必填)
	</div>

	<div class="col-md-6 col-lg-4">
		<label>企业名称：</label>
		<input type="text" class="form-control" name="companyName" ng-model="customer.companyName">
	</div>		
	<div class="col-md-6 col-lg-4">
		<label>营业执照号：</label>
		<input type="text" class="form-control" name="licenseNumber" ng-model="customer.licenseNumber">
	</div>
	<div class="col-md-6 col-lg-4">
		<label>法定代表人：</label>
		<input type="text" class="form-control" name="representative" ng-model="customer.representative">
	</div>
		
	<div class="col-md-6 col-lg-4">
		<label>注册资本：</label>
		<input type="number" min="0" onkeyup="if(value<0)value=Math.abs(value);" class="form-control" name="registeredCapital" ng-model="customer.registeredCapital">万元
	</div>
	<div class="col-md-6 col-lg-4">
		<label>成立时间：</label>
		<input type="text" class="form-control form_date" name="establishTime" ng-model="customer.establishTime">
	</div>
	<div class="col-md-6 col-lg-4">
		<label>员工数量：</label>
		<input type="number" min="0" onkeyup="if(value<0)value=Math.abs(value);" class="form-control" name="employeesNumber" ng-model="customer.employeesNumber">人
	</div>
	
	<div class="col-md-6 col-lg-4">
		<label>行业类别：</label>
		<input type="text" class="form-control" name="category" ng-model="customer.category">
	</div>
	<div class="col-md-6 col-lg-8">
		<label>实际经营地址：</label>
		<input type="text" class="form-control" name="operatingAddress" ng-model="customer.operatingAddress" style="width:67%;">
	</div>
	
	<div class="col-md-12 detail-right-tit ">&nbsp;&nbsp;股东信息
		<button type="button" class="btn btn-info detail-right-save fr" ng-click="shareholderAdd()">+ 企业股东信息</button>
	</div>	
	<div ng-repeat="obj in customer.customerShareholderDto" >
		<div class="col-md-12 danbao-tit">&nbsp;&nbsp; 股东信息（<span class="num-box" ng-bind="$index+1"></span>）
			<span class="detail-trash text-danger" ng-if="$index>0" ng-click="shareholderDelete($index)">删除</span>
		</div>

		<div class="col-md-6 col-lg-4"><label>股东姓名：</label>
			<input type="text" class="form-control" name="shareholderName{{index}}" ng-model="obj.shareholderName">
		</div>
		<div class="col-md-6 col-lg-4"><label>出资方式：</label>
			<input type="text" class="form-control" name="contributionMethod{{index}}" ng-model="obj.contributionMethod">
		</div>
		<div class="col-md-6 col-lg-4"><label>出资额：</label>
			<input type="number" min="0" onkeyup="if(value<0)value=Math.abs(value);" class="form-control" name="contributionAmount{{index}}" ng-model="obj.contributionAmount">万元
		</div>

		<div class="col-md-6 col-lg-4"><label>出资比例：</label>
			<input type="number" min="0" onkeyup="if(value<0)value=Math.abs(value);" class="form-control" name="contributionProportion{{index}}" ng-model="obj.contributionProportion">%
		</div>
	</div>
</form>	
<!--企业信息End-->