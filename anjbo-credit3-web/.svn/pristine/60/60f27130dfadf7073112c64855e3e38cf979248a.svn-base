<style>
    .form-inline.w112 label{width:112px;}
    .form-inline.w154 label{width:154px;}
    .form-inline.w154 .form-control{width:150px}
</style>
<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;申请预约信息                 <!--style="float:right;" 按钮右移 -->
    <button type="button" ng-click="saveA(1)" class="btn btn-info detail-right-save"  >保存</button>
    </div>
    <div class="row form-inline detail-row" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4 ">
            <label>申请人姓名: </label> <input ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" type="text" name="appointCustName" ng-model="huaRongDto.lcAppoint.custName" class="form-control" required/>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointCustName.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>证件类型:</label>
            <select ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" class="form-control"name="appointIdType" ng-disabled="all" ng-model="huaRongDto.lcAppoint.idType" required style="width:150px;">
                <option value="">请选择</option>
                <option value="身份证">身份证</option>
                <option value="军官证">军官证</option>
                <option value="台湾同胞来往内地通行证">台湾同胞来往内地通行证</option>
                <option value="港澳居民来往内地通行证">港澳居民来往内地通行证</option>
                <option value="员工代码">员工代码</option>
                <option value="其他证件">其他证件</option>
            </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointIdType.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>证件号:</label> <input ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" type="text" name="appointIdNo" ng-model="huaRongDto.lcAppoint.idNo" class="form-control" required/>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointIdNo.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>手机号:</label> <input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" name="appointMobile" ng-model="huaRongDto.lcAppoint.mobile"  class="form-control" required/>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointMobile.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>还款方式:</label>
            <select class="form-control" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" name="appointMtdCde" ng-disabled="all" ng-model="huaRongDto.lcAppoint.mtdCde" required style="width:150px;">
                <option value="">请选择</option>
                <option value="按月分期还款">按月分期还款</option>
                <option value="等额本金还款">等额本金还款</option>
                <option value="利随本清">利随本清</option>
                <option value="按月还息到期还本付息">按月还息到期还本付息</option>
                <option value="按月还息+等额本息(10+12)">按月还息+等额本息(10+12)</option>
                <option value="按日计息,利随本清">按日计息,利随本清</option>
            </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointMtdCde.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>贷款申请金额:</label><input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" string-to-number name="appointApplyAmt" ng-model="huaRongDto.lcAppoint.applyAmt"  class="form-control" required/> 万元
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointApplyAmt.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>申请期限:</label><input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" string-to-number name="appointApplyTnr" ng-model="huaRongDto.lcAppoint.applyTnr"  class="form-control" required/><span ng-bind="huaRongDto.lcAppoint.applyTnrUnit"></span>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointApplyTnr.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>借款利率:</label><input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" string-to-number name="appointApr" ng-model="huaRongDto.lcAppoint.apr"  class="form-control" required/>&nbsp;%/天
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointApr.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>借款用途:</label>
            <select class="form-control" name="appointUse" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'"  ng-disabled="all" ng-model="huaRongDto.lcAppoint.useHr" required style="width:150px;">
                <option value="">请选择</option>
                <option value="短期周转">短期周转</option>
                <option value="生意周转">生意周转</option>
                <option value="生活周转">生活周转</option>
                <option value="购物消费">购物消费</option>
                <option value="不提现借款">不提现借款</option>
                <option value="其它借款">其它借款</option>
                <option value="创业借款">创业借款</option>
                <option value="教育培训借款">教育培训借款</option>
            </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointUse.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>借款期限单位:</label>
            <select class="form-control" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" name="appointApplyTnrUnit" ng-disabled="all" ng-model="huaRongDto.lcAppoint.applyTnrUnit" required style="width:150px;">
                <option value="">请选择</option>
                <option value="月">月</option>
                <option value="日">日</option>
            </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointUse.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>审批通过金额:</label><input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" string-to-number name="appointApprvAmt" ng-model="huaRongDto.lcAppoint.apprvAmt"  class="form-control" required/> 万元
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.appointApprvAmt.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>产品编号:</label>
             <select name="productId" ng-model="huaRongDto.lcAppoint.productId" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" class="form-control" required>
             		<option value="KG001">KG001</option>
             		<option value="KG002">KG002</option>
             </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.productId.$error.required">必选</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>助贷商编号:</label>
             <select name="loanCooprCode" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" ng-model="huaRongDto.lcAppoint.loanCooprCode"  class="form-control" required>
             		<option value="KG" ng-if="huaRongDto.lcAppoint.productId=='KG001'" >KG</option>
             		<option value="KG2" ng-if="huaRongDto.lcAppoint.productId=='KG002'">KG2</option>
             </select>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.loanCooprCode.$error.required">必选</error>
		    </span>
        </div>
    </div>
</div>


<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;申请人信息</div>
    <div class="row form-inline detail-row w112" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>申请人证件号: </label> <input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" ng-model="huaRongDto.lcApplAppt.idNo" name="applApptIdNo" class="form-control" required/>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.applApptIdNo.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>申请人证件类型: </label>
            <select class="form-control" name="applApptIdType" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'"  ng-disabled="all" ng-model="huaRongDto.lcApplAppt.idType" required style="width:150px;">
                <option value="">请选择</option>
                <option value="身份证">身份证</option>
                <option value="军官证">军官证</option>
                <option value="台湾同胞来往内地通行证">台湾同胞来往内地通行证</option>
                <option value="港澳居民来往内地通行证">港澳居民来往内地通行证</option>
                <option value="员工代码">员工代码</option>
                <option value="其他证件">其他证件</option>
            </select>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.applApptIdType.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>申请人名称: </label> <input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" ng-model="huaRongDto.lcApplAppt.custName " name="applApptCustName" class="form-control" required/>
             <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.applApptCustName.$error.required">必填</error>
		    </span>
        </div>
    </div>
</div>

<div>
    <div class="col-md-12 detail-right-tit">&nbsp;&nbsp;申请人详细信息</div>
    <div class="row form-inline detail-row" style="padding:0 10px;">
        <div class="col-md-6 col-lg-4">
            <label>姓名: </label> <input type="text" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" name="apptIndivCustName" ng-model="huaRongDto.lcApptIndiv.custName" class="form-control" required/>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.apptIndivCustName.$error.required">必填</error>
		    </span>
        </div>
        <div class="col-md-6 col-lg-4">
            <label>教育程度: </label>
            <select class="form-control" name="apptIndivIndivEdt" ng-disabled="dealStatus=='审批通过'||dealStatus=='待审批'" ng-model="huaRongDto.lcApptIndiv.indivEdt" required style="width:150px;">
                <option value="">请选择</option>
                <option value="初中及以下">初中及以下</option>
                <option value="高中,中专及技校">高中,中专及技校</option>
                <option value="大专">大专</option>
                <option value="本科">本科</option>
                <option value="硕士">硕士</option>
                <option value="博士及以上">博士及以上</option>
                <option value="未知">未知</option>
            </select>
            <span class="inputError" ng-show="isAudit">
					<error class="text-danger" ng-show="huarongAppointForm.apptIndivIndivEdt.$error.required">必填</error>
		     </span>
        </div>
    </div>
</div>