<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <ul class="breadcrumb">
                <li>
                    <a>要件管理</a>
                    <span class="divider"></span>
                </li>
                <li class="active">
                    <a ui-sref="boxList">柜子管理</a>
                    <span class="divider"></span>
                </li>
                <li class="active">开箱记录</li>
                <a style="display:inline-block;float:right;padding-right: 10px;" ui-sref="boxList" >返回上一页</a>
            </ul>
        </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <form class="form-inline" style="margin-bottom:20px;margin-left:30px; ">
                <div class="form-group">
                    <label>时间：</label>
                    <input size="16" type="text" name="startTime" ng-model="page.startTime " class="form-control form_date">
                    -
                    <input size="16" type="text" name="endTime" ng-model="page.endTime " class="form-control form_date">
                </div>
                <div class="form-group">
                    <label >申请人：</label>
                    <input type="text" class="form-control" maxlength="15" ng-model="page.applierName">
                </div>
                <div class="form-group">
                    <label >操作人：</label>
                    <input type="text" class="form-control" maxlength="15" ng-model="page.currentHandler">
                </div>
                <button type="submit" class="btn btn-default" ng-click="query()" style="margin-bottom:10px; margin-right:10px;">
                    查询
                </button>
                <button type="submit" class="btn btn-default" ng-click="reset()" style="margin-bottom:10px; margin-right:10px;">
                    重置
                </button>

            </form>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table bs-table-control="openBoxList"  id="table"></table>
        </div>
    </div>
</div>