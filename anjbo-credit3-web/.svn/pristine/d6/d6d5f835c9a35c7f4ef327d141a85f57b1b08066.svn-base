<div class="container-fluid" id="element">
	<!--标题-->
	<div class="row">
		<div class="col-md-12 detail-right-tit">&nbsp;&nbsp;要件校验信息
			<a ng-show="!isEdit" style="display:inline-block;float:right;padding-right: 10px;" ui-sref="orderList" >返回上一步</a>
			<span ng-show="isEdit" class="detail-trash2 text-danger" ng-click="deleteDetail('element-detail')" >隐藏</span>
			<div style="position:absolute;right:400px;top:0px;">
			  <span id="elementFlag" ng-bind="elementFlag">是否已收到要件：是</span>
			  <span><a id="elementInfo" ng-bind="elementInfo" ng-click="showDetail('elementEntrance');">要件详情</a></span>
			</div>
		</div>
	</div>
	<!--标题 end-->
	<div class="row detail-row detail-lhw" >
		<div class="col-md-6 col-lg-4"><label>校验方式：</label><span ng-if="obj.status==2">通过钉钉特批</span><span ng-if="obj.status==3">通过系统校验</span></div>
	</div>
	<!--备注-->
	<!---->
	<div class="row relative" ng-if="obj.remark!=null && obj.remark!=''">
		<span class="remark-span remark-lhw" >备注:</span>
		<textarea  ng-bind="obj.remark"   class="remark-p remark-lhw-p textarea-lhw" readonly="readonly"/>
	</div> 
	<br>
	<!--备注end-->

	<!--图片 放大插件-->
	<div class="row gallerys" ng-if="obj.imgUrl!=null&&obj.imgUrl!=undefined&&obj.imgUrl!=''">
		<img src="{{img}}" width="150" height="150" class="detail-img gallery-pic" ng-repeat="img in obj.imgUrl">
	</div>
	<!--图片end-->
	
	
	<!--图片 放大插件-->
	<div class="row gallerys" ng-if="obj.elementUrls!=null&&obj.elementUrls!=undefined&&obj.elementUrls!=''">
		<img src="{{img}}" width="150" height="150" class="detail-img gallery-pic" ng-repeat="img in obj.elementUrls">
	</div>
	<!--图片end-->
	
</div>