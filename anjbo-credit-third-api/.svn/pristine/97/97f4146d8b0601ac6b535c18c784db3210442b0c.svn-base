<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.anjbo.dao.baidu.BaiduFaceMapper">

	<insert id="addBaiduFaceData" parameterType="java.util.Map">
		insert into tbl_third_baidu_face_data (
		    uid,
		    idCardNo,
		    realName,
		    imageUrl,
		    score,
		    imageBase64,
		    appSource,
		    city,
		    device,
		    terminalType,
		    osType,
		    version
		)values(
		    #{uid},
		    #{idCardNo},
		    #{realName},
		    #{imageUrl},
		    #{score},
		    #{imageBase64},
		    #{appSource},
		    #{city},
		    #{device},
		    #{terminalType},
		    #{osType},
		    #{version}
		)
	</insert>
	
</mapper>