<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.anjbo.dao.risk.AuditFinalMapper" >
  <sql id="sqlwhereSearch">
  	<where>
		<if test="null!=id and id>0"> AND `id`=#{id}</if>
		<if test="null!=createUid and ''!=createUid"> AND `createUid`=#{createUid}</if>
		<!--
		<if test="@Ognl@isNotEmpty(createTimeBegin)"> AND createTime >= #{createTimeBegin}</if>
		<if test="@Ognl@isNotEmpty(createTimeEnd)"> AND createTime &lt;= #{createTimeEnd}</if>
		-->
		<if test="null!=updateUid and ''!=updateUid"> AND `updateUid`=#{updateUid}</if>
		<!--
		<if test="@Ognl@isNotEmpty(updateTimeBegin)"> AND updateTime >= #{updateTimeBegin}</if>
		<if test="@Ognl@isNotEmpty(updateTimeEnd)"> AND updateTime &lt;= #{updateTimeEnd}</if>
		-->
		<if test="null!=orderNo and ''!=orderNo"> AND `orderNo`=#{orderNo}</if>
		<!--
		<if test="@Ognl@isNotEmpty(auditTimeBegin)"> AND auditTime >= #{auditTimeBegin}</if>
		<if test="@Ognl@isNotEmpty(auditTimeEnd)"> AND auditTime &lt;= #{auditTimeEnd}</if>
		-->
		<if test="null!=remark and ''!=remark"> AND `remark`=#{remark}</if>
		<if test="null!=isShow and isShow>0"> AND `isShow`=#{isShow}</if>
		<if test="null!=auditStatus and auditStatus>0"> AND `auditStatus`=#{auditStatus}</if>
		<if test="null!=handleUid and ''!=handleUid"> AND `handleUid`=#{handleUid}</if>
		<if test="null!=paymentType and ''!=paymentType"> AND `paymentType`=#{paymentType}</if>
		<if test="null!=officerUid and ''!=officerUid"> AND `officerUid`=#{officerUid}</if>
		<if test="null!=officerUidType and officerUidType>0"> AND `officerUidType`=#{officerUidType}</if>
		<if test="null!=allocationFundUid and ''!=allocationFundUid"> AND `allocationFundUid`=#{allocationFundUid}</if>
	</where>
  </sql>
    

  <!-- 获取实体 -->
  <select id="find" parameterType="AuditFinalDto" resultType="AuditFinalDto">    
	SELECT `id`, `createUid`, `createTime`, `updateUid`, `updateTime`, `orderNo`, `auditTime`, `remark`, `isShow`, `auditStatus`, `handleUid`, `paymentType`, `officerUid`, `officerUidType`, `allocationFundUid` 
	FROM tbl_risk_audit_final 
	<include refid="sqlwhereSearch"/>
  </select>

  <!-- 查询 -->
  <select id="search" parameterType="AuditFinalDto" resultType="AuditFinalDto">
	SELECT `id`, `createUid`, `createTime`, `updateUid`, `updateTime`, `orderNo`, `auditTime`, `remark`, `isShow`, `auditStatus`, `handleUid`, `paymentType`, `officerUid`, `officerUidType`, `allocationFundUid`
	FROM tbl_risk_audit_final 
	<include refid="sqlwhereSearch"/>

	<if test="pageSize > 0 ">limit #{start},#{pageSize}</if>
  </select>

  <!-- 查询 -->
  <select id="count" parameterType="AuditFinalDto" resultType="int">
	SELECT count(1) 
	FROM tbl_risk_audit_final 
	<include refid="sqlwhereSearch"/> 
  </select>
  
    
  <!-- 增删改 -->
  <insert id="insert" parameterType="AuditFinalDto" useGeneratedKeys="true" keyProperty="id">
    INSERT INTO tbl_risk_audit_final(
		`createUid`,
		`createTime`,
		`updateUid`,
		`updateTime`,
		`orderNo`,
		`auditTime`,
		`remark`,
		`isShow`,
		`auditStatus`,
		`handleUid`,
		`paymentType`,
		`officerUid`,
		`officerUidType`,
		`allocationFundUid`
	) VALUES(
		#{createUid},
		now(),
		#{createUid},
		now(),
		#{orderNo},
		#{auditTime},
		#{remark},
		#{isShow},
		#{auditStatus},
		#{handleUid},
		#{paymentType},
		#{officerUid},
		#{officerUidType},
		#{allocationFundUid}
	)	
  </insert>


  <delete id="delete" parameterType="AuditFinalDto" >
    DELETE FROM tbl_risk_audit_final 
	<include refid="sqlwhereSearch"/>
  </delete>
  
  <update id="update" parameterType="AuditFinalDto" >
    UPDATE tbl_risk_audit_final
	<set>
		<if test="null!=createTime">`createTime`=now(),</if>
		<if test="null!=updateUid">`updateUid`=#{updateUid},</if>
		<if test="null!=updateTime">`updateTime`=now(),</if>
		<if test="null!=orderNo">`orderNo`=#{orderNo},</if>
		<if test="null!=auditTime">`auditTime`=#{auditTime},</if>
		<if test="null!=remark">`remark`=#{remark},</if>
		<if test="null!=isShow">`isShow`=#{isShow},</if>
		<if test="null!=auditStatus">`auditStatus`=#{auditStatus},</if>
		<if test="null!=handleUid">`handleUid`=#{handleUid},</if>
		<if test="null!=paymentType">`paymentType`=#{paymentType},</if>
		<if test="null!=officerUid">`officerUid`=#{officerUid},</if>
		<if test="null!=officerUidType">`officerUidType`=#{officerUidType},</if>
		<if test="null!=allocationFundUid">`allocationFundUid`=#{allocationFundUid},</if>
	</set>
	WHERE orderNo=#{orderNo}
  </update>
    
</mapper>