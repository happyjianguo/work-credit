<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<ul class="breadcrumb">
				<li>
					<a>字典管理 </a> <span class="divider"></span>
				</li>
				<li class="active">
					字典列表
				</li>
			</ul>
		</div>
	</div>
	<!--row-->
	<div class="row">
		<div class="col-md-12"> 
		    <form class="form-inline" style="margin-left:30px;">
			     <div class="form-group">
			        <label>字典类型：</label>
			        <input type="text" class="form-control" ng-model="page.type" />
			    </div>
			    <div class="form-group">
			        <label>字典名称：</label>
			        <input type="text" class="form-control" ng-model="page.name" />
			    </div>
		    	<button type="submit" class="btn btn-default" ng-click="query()" style="margin-right:10px; margin-bottom: 10px;">查询</button>
		    	<button ng-click="addDict()" type="button" class="btn btn-default"  style="margin-right:10px; margin-bottom: 10px;">新增</button>
			</form>
		</div>
	</div>
	<!--row-->
	<div class="row">
		<div class="col-md-12">
			<!--table-->
			<table bs-table-control="dictList" id="table"></table>
			<!--table end-->
	    </div>
	</div>
</div>

 