<div class="container-fluid">
	<div class="row">
		<div class="col-md-12">
			<ul class="breadcrumb">
				<li>
					<a>资方管理 </a> <span class="divider"></span>
				</li>
				<li class="active">
					资金方列表
				</li>
			</ul>
		</div>
	</div>
	
	<!--row-->
	<div class="row">
		<div class="col-md-12"> 
		    <form class="form-inline" style="margin-left:30px;">
				<div class="form-group">
					<label >资金方：</label>
					<input type="text" class="form-control" ng-model="page.fundName">
				</div>
				<button type="submit" class="btn btn-default" ng-click="query()" style="margin-bottom:10px; margin-right:10px;">查询</button>
				<button type="button" ng-if="meunAuth.indexOf('auth15auth')>=0 " class="btn btn-primary fr" ui-sref="fundEdit({'fundId':'-1'})" style="margin-right:10px;   margin-bottom: 10px;">添加资金方</button>
			</form>
		</div>
	</div>
	<!--row-->

	<div class="row">
		<div class="col-md-12">
			<!--table-->
			<table bs-table-control="fundList" id="table"></table>
			<!--table end-->
	    </div>
	</div>
</div>

 