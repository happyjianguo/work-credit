package com.anjbo.bean.tools;

public class MonthlyItemInfo {
	//期次
	private int index;
	//期次说明
	private String indexDetail;
	//每月偿还利息
	private String monthlyInterest;
	//每月偿还本金
	private String monthlyPrincipal;
	//每月还款额
	private String monthlyPayments;
	//剩余本金
	private String remainder;
	public int getIndex() {
		return index;
	}
	public void setIndex(int index) {
		this.index = index;
	}
	public String getIndexDetail() {
		return indexDetail;
	}
	public void setIndexDetail(String indexDetail) {
		this.indexDetail = indexDetail;
	}
	public String getMonthlyInterest() {
		return monthlyInterest;
	}
	public void setMonthlyInterest(String monthlyInterest) {
		this.monthlyInterest = monthlyInterest;
	}
	public String getMonthlyPrincipal() {
		return monthlyPrincipal;
	}
	public void setMonthlyPrincipal(String monthlyPrincipal) {
		this.monthlyPrincipal = monthlyPrincipal;
	}
	public String getMonthlyPayments() {
		return monthlyPayments;
	}
	public void setMonthlyPayments(String monthlyPayments) {
		this.monthlyPayments = monthlyPayments;
	}
	public String getRemainder() {
		return remainder;
	}
	public void setRemainder(String remainder) {
		this.remainder = remainder;
	}
	@Override
	public String toString() {
		return "\nItemInfo [期数=" + index + ", 期数年月=" + indexDetail
				+ ", 应还利息=" + monthlyInterest + ", 应还本金=" + monthlyPrincipal
				+ ", 还款额=" + monthlyPayments + ", 剩余本金=" + remainder + "]";
	}
	
}
